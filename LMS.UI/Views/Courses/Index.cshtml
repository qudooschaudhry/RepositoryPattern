@model IEnumerable<LMS.UI.Models.CourseItemModel>

<h1>Courses</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Course Name</th>
            <th scope="col">Start Date</th>
            <th scope="col">End Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td><a href="/courses/edit/@course.Id">@course.Name</a></td>
                <td>@course.Startdate</td>
                <td>@course.EndDate</td>
            </tr>
        }
    </tbody>
</table>
